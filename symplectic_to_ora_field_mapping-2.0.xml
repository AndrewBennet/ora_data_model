    <!-- 
        ===========================
        TYPE OF WORK MAPPINGS
        ===========================
        
        This section details how to map work types, especially in the context of the Sword2 Hyrax-Work-Type header
        
        symplectic_work_type = name of Symplectic type of work
        symplectic_sub_type = name of Symplectic sub type, corresponds to the value set at '//pubs:field[@name="types"][@display-name="Sub types']'
        ora_work_type = name of ORA type of work (field:type_of_work), and name of Hyrax Work Type to be set in the Hyrax-Work-Type header
        ora_sub_type_of_work = name of ORA sub type of work (field:sub_type_of_work), see also the Symplectic 'types' field mapping instructions above
    -->
    
    <work-type-mappings>
        <!-- Books (book, scholarly edition, edited book, translation -->
        <symplectic_work_type>book
